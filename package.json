{
  "name": "agent-file",
  "version": "1.0.4",
  "description": "Self-contained HTML agent format with built-in security",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "agent-html": "./src/cli.ts"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "bun test",
    "dev": "tsc --watch",
    "example:vanilla": "bun run examples/vanilla/simple-agent.ts",
    "example:langgraph": "bun run examples/langchain/langgraph-agent.ts",
    "example:litert": "bun run examples/litert/simple-agent.ts",
    "example:huggingface": "bun run examples/huggingface/simple-agent.ts",
    "example:wasi": "bun run examples/advanced/wasi/wasi-agent.ts",
    "example:mcp": "bun run examples/mcp/mcp-agent.ts",
    "mcp:server": "bun run examples/mcp/test-mcp-server.ts"
  },
  "keywords": [
    "ai",
    "agent",
    "html",
    "self-contained",
    "portable",
    "security",
    "langchain",
    "langgraph",
    "mcp",
    "model-context-protocol"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@langchain/core": ">=0.1.0",
    "@langchain/langgraph": ">=0.0.1"
  },
  "peerDependenciesMeta": {
    "@langchain/core": {
      "optional": true
    },
    "@langchain/langgraph": {
      "optional": true
    }
  }
}
